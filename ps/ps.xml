				</parameters>
			</button_stringcycler>
			<label_fieldtop>
				<anchored to="savedcbase" />
				<static textres="power_label_savedcbase" />
			</label_fieldtop>
			<basicnumber name="savedcmod">
				<anchored to="savedcbase" position="righthigh" offset="10,0" width="25" height="20" />
